{
    "highlight_selected_line": true,
    "panes": {
        "quadrants": [
            "Source",
            "TabSet2",
            "TabSet1",
            "Console"
        ],
        "tabSet1": [
            "Environment",
            "Files",
            "Plots",
            "Connections",
            "Packages",
            "Help",
            "Build",
            "VCS",
            "Tutorial",
            "Viewer",
            "Presentation"
        ],
        "tabSet2": [
            "History"
        ],
        "console_left_on_top": false,
        "console_right_on_top": false
    },
    "show_indent_guides": true,
    "soft_wrap_r_files": true,
    "syntax_color_console": true,
    "highlight_r_function_calls": true,
    "editor_theme": "Kiss",
    "default_project_location": "~/github/rileytwo/rpackages",
    "source_with_echo": true,
    "default_sweave_engine": "knitr",
    "pdf_previewer": "rstudio",
    "save_workspace": "never",
    "remove_history_duplicates": true,
    "reuse_sessions_for_project_links": true,
    "posix_terminal_shell": "zsh",
    "continue_comments_on_newline": true,
    "fold_style": "begin-only",
    "full_project_path_in_window_title": true,
    "jobs_tab_visibility": "closed",
    "custom_shell_command": "/usr/local/bin/zsh",
    "custom_shell_options": "-l",
    "terminal_bell_style": "none",
    "new_proj_git_init": true,
    "check_arguments_to_r_function_calls": true,
    "warn_if_no_such_variable_in_scope": true,
    "warn_variable_defined_but_not_used": true,
    "style_diagnostics": true,
    "check_unexpected_assignment_in_function_call": true,
    "rmd_viewer_type": "pane",
    "show_hidden_files": true,
    "strip_trailing_whitespace": true,
    "disabled_aria_live_announcements": [
        "console_cleared",
        "console_log",
        "console_command",
        "filtered_list",
        "git_message_length",
        "inaccessible_feature",
        "info_bar",
        "progress_completion",
        "progress_log",
        "screen_reader_not_enabled",
        "session_state",
        "tab_key_mode",
        "toolbar_visibility",
        "warning_bar"
    ],
    "help_font_size_points": 11
}